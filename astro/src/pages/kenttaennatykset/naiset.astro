      class="mx-auto my-8 max-w-7xl px-4 sm:my-12 sm:px-8 lg:my-16 lg:grid lg:grid-cols-5 lg:px-12"
    >
      <!-- LEAGUE SIDEBAR NAVIGATION -->
      <nav class="hidden lg:block">
        <ul
          class="space-y-1 text-sm font-medium text-gray-500"
          aria-label="Sidebar"
        >
          <li>
            <a
              href="/kenttaennatykset/miehet#main"
              rel="prefetch"
              class="block rounded-md px-3 py-2 hover:bg-white hover:text-gray-900"
            >
              Miehet
            </a>
          </li>
          <li>
            <a
              href="/kenttaennatykset/naiset#main"
              rel="prefetch"
              class="block rounded-md bg-red-100 px-3 py-2 text-red-800"
            >
              Naiset
            </a>
          </li>
        </ul>
